<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>CS_Money</RootNamespace>
    <Authors>Rmixs</Authors>
    <Copyright></Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Rm1xs/CS-Money-Library</RepositoryUrl>
    <PackageIcon>icon144x144.jpg</PackageIcon>
    <PackageTags>.NET, CS.Moeny, Items, Skins, Price, Parser, Library</PackageTags>
    <Description>Library for getting all current skins on cs.money including the price, float, quantity etc.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\icon144x144.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
